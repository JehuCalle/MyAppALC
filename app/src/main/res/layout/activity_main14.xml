<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fondo"
    tools:context=".MainActivity14">

    <TextView
        android:id="@+id/textViewHotel2"
        android:layout_width="353dp"
        android:layout_height="66dp"
        android:background="#B2A8D4E1"
        android:backgroundTint="#7287D1E8"
        android:text="Deje su comentario Abajo"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textColor="#C56626"
        android:textColorHighlight="#BC1A1A"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.087" />

    <Button
        android:id="@+id/btnRegresarComenHtel2"
        android:layout_width="85dp"
        android:layout_height="34dp"
        android:backgroundTint="#BCCEF3"
        android:text="Regresar"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textColor="#007A53"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.047"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.977" />
</androidx.constraintlayout.widget.ConstraintLayout>